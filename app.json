{
  "expo": {
    "name": "Panggilin",
    "slug": "panggilin-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/panggilin.png",
    "scheme": "panggilin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "mailto",
          "message",
          "readdle-spark",
          "airmail",
          "ms-outlook",
          "googlegmail",
          "inbox-gmail",
          "ymail",
          "superhuman",
          "yandexmail",
          "fastmail",
          "protonmail"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/panggilin.png",
        "backgroundColor": "#7E57C2"
      },
      "package": "com.artadev.panggilin"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/panggilin.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/panggilin.png",
          "resizeMode": "cover",
          "backgroundColor": "#7E57C2"
        }
      ],
      "react-native-email-link"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "33d31597-7556-46e3-875e-9da0e5f87e08"
      }
    },
    "owner": "3snanugraha"
  }
}
